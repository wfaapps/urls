{
  "version": 1,
  "allowed": [
    "https://*.gov.in/*",
    "https://*.nic.in/*",
    "https://*.ecourts.gov.in/*",
    "https://services.ecourts.gov.in/"
  ],
  "max_upload_size_mb": 1,
  "allowed_extensions": [
    ".pdf",
    ".docx",
    ".doc",
    ".jpg",
    ".png",
    ".eml",
    ".mp3",
    ".wav",
    ".aac"
  ],
  "clipboard_max_paste_bytes": 1024,
  "clipboard_restriction_enabled": true
}
